{
	"name": "auth0-socketio",
	"version": "1.1.1",
	"description": "A near-zero config middleware to use Auth0 JWT on your Socket.IO server",
	"main": "build/index.js",
	"type": "module",
	"scripts": {
		"test": "jest",
		"watcher": "jest --watch",
		"build": "tsc",
		"start": "node . ",
		"lint": "eslint src --ext 'ts,json'",
		"dev": "tsc-watch --onSuccess 'node .' ",
		"prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/scristobal/auth0-socketio.git"
	},
	"keywords": [
		"auth0",
		"jwt",
		"socketio",
		"socket.io",
		"authorization"
	],
	"author": "Samuel Cristobal",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/scristobal/auth0-socketio/issues"
	},
	"homepage": "https://github.com/scristobal/auth0-socketio#readme",
	"dependencies": {
		"dotenv": "^16.0.0",
		"jose": "^4.4.0",
		"onchange": "^7.1.0",
		"prettier": "^2.7.1"
	},
	"devDependencies": {
		"@tsconfig/recommended": "^1.0.1",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.15",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"eslint": "^8.8.0",
		"eslint-watch": "^8.0.0",
		"jest": "^27.5.0",
		"socket.io": "^4.4.1",
		"ts-jest": "^27.1.3",
		"tsc-watch": "^4.6.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	}
}
